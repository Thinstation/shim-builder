for dir in `ls --color=never /usr/ports/`;do 
	for port in `ls --color=never /usr/ports/$dir`; do 
		if ! prt-get isinst $port 1>/dev/null; then 
			echo $port;
		fi;
	done;
done
for dir in `ls --color=never /ts/ports/`;do 
	for port in `ls --color=never /ts/ports/$dir`; do 
		if ! prt-get isinst $port 1>/dev/null; then 
			echo $port;
		fi;
	done;
done
